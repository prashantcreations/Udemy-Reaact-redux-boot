package com.example.demo.payload;

public class JWTLoginSuccessResponse {
	private boolean success;
	private String token;
	
	
	public JWTLoginSuccessResponse(boolean success, String response) {
		super();
		this.success = success;
		this.token = response;
	}
	public boolean isSuccess() {
		return success;
	}
	public void setSuccess(boolean success) {
		this.success = success;
	}
	public String getResponse() {
		return response;
	}
	public void setResponse(String response) {
		this.response = response;
	}
	@Override
	public String toString() {
		return "JWTLoginSuccessResponse [success=" + success + ", response=" + response + ", isSuccess()=" + isSuccess()
				+ ", getResponse()=" + getResponse() + ", getClass()=" + getClass() + ", hashCode()=" + hashCode()
				+ ", toString()=" + super.toString() + "]";
	}
}
